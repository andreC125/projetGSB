<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="projetGSB.pages.AccueilRegion">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="btnInsererRegion" Clicked="BtnInsererRegion_ClickedAsync" Text="inserer une region"></Button>
            <Button x:Name="btnModifierRegion" Clicked="BtnModifierRegion_ClickedAsync" Text="modifier une region"></Button>
            <Button x:Name="btnListerRegion" Clicked="BtnListerRegion_ClickedAsync" Text="liste des regions"></Button>
            <Button x:Name="btnRetour" Clicked="BtnRetour_ClickedAsync" Text="retour"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>