<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="projetGSB.pages.InsererVisiteur">



    <ContentPage.Content>


        <StackLayout>
            <Label>inserer un visiteur</Label>
            <Label>region ou il travaille</Label>
            <Picker x:Name="lstRegion" SelectedIndexChanged="LstRegion_SelectedIndexChanged" Title="les régions"></Picker>
            <DatePicker x:Name="date"></DatePicker>
            <TimePicker x:Name="time"></TimePicker>
            <Button Text="retour" Clicked="BtnRetour_ClickedAsync"  x:Name="btnRetour"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>